<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
	<PreFlow/>
  	<Flows/>
  	<PostFlow/>
	<HTTPProxyConnection>
		<BasePath>/v1/recommended</BasePath>
		<VirtualHost>secure</VirtualHost>
